<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>FinalQuote</servlet-name>
  	<display-name>FinalQuote</display-name>
  	<description></description>
  	<servlet-class>com.arken.finalquote.web.FinalQuote</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CustomerQuote</servlet-name>
  	<display-name>CustomerQuote</display-name>
  	<description></description>
  	<servlet-class>com.arken.customerquote.web.CustomerQuote</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>FinalQuote</servlet-name>
  	<url-pattern>/FinalQuote</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CustomerQuote</servlet-name>
  	<url-pattern>/CustomerQuote</url-pattern>
  </servlet-mapping>
</web-app>